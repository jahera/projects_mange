<h2>Projects</h2>
<table>
  <thead>
    <tr>
      <th>Project Name</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.title %></td>
        <td>
        <td><%#= link_to 'Show', task %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%#= link_to 'Destroy', task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>